<?xml version="1.0"?>
<tryton>
    <data>

<!-- SchoolPaymentIncome -->
        <record model="ir.ui.view" id="school_account_status_context_view_form">
            <field name="model">school.account.status.context</field>
            <field name="type">form</field>
            <field name="name">school_account_status_context_form</field>
        </record>

<!-- SchoolPaymentWizardStart -->
        <record model="ir.ui.view" id="school_payment_wizard_start_view_form">
            <field name="model">school.payment.wizard.start</field>
            <field name="type">form</field>
            <field name="name">school_payment_wizard_start_form</field>
        </record>

        <record model="ir.ui.view" id="school_payment_wizard_voucher_error_view_form">
            <field name="model">school.payment.wizard.start</field>
            <field name="type">form</field>
            <field name="name">school_payment_wizard_voucher_error_form</field>
        </record>
        <record model="ir.ui.view" id="school_payment_wizard_pay_ok_view_form">
            <field name="model">school.payment.wizard.start</field>
            <field name="type">form</field>
            <field name="name">school_payment_wizard_pay_ok_form</field>
        </record>

<!-- SchoolPaymentWizard -->
        <record model="ir.action.wizard" id="act_school_payment_wizard">
            <field name="name">Payment</field>
            <field name="wiz_name">school.payment.wizard</field>
            <field name="window" eval="True"/>
        </record>
        <menuitem
            parent="menu_school"
            sequence="40"
            action="act_school_payment_wizard"
            id="menu_payment_wizard"/>
        <record model="ir.ui.menu-res.group" id="menu_payment_wizard_group_admin">
            <field name="menu" ref="menu_payment_wizard"/>
            <field name="group" ref="res.group_admin"/>
        </record>
        <record model="ir.ui.menu-res.group" id="menu_payment_wizard_group_school_admin">
            <field name="menu" ref="menu_payment_wizard"/>
            <field name="group" ref="group_school_admin"/>
        </record>

<!-- Schoolpayment -->
        <record model="ir.ui.view" id="school_payment_view_form">
            <field name="model">school.payment</field>
            <field name="type">form</field>
            <field name="name">school_payment_form</field>
        </record>
        <record model="ir.ui.view" id="school_payment_view_list1">
            <field name="model">school.payment</field>
            <field name="type">tree</field>
            <field name="name">school_payment_list</field>
        </record>

        <record model="ir.action.act_window" id="act_school_payment">
            <field name="name">School payment</field>
            <field name="res_model">school.payment</field>
        </record>
        <record model="ir.action.act_window.domain"
                id="act_school_payment_domain_paid">
            <field name="name">Paid</field>
            <field name="sequence" eval="10"/>
            <field name="domain" eval="[('state', '=', 'paid')]" pyson="1"/>
            <field name="act_window" ref="act_school_payment"/>
        </record>
        <record model="ir.action.act_window.domain"
                id="act_school_payment_domain_must">
            <field name="name">Must</field>
            <field name="sequence" eval="20"/>
            <field name="domain" eval="[('state', '=', 'must')]" pyson="1"/>
            <field name="act_window" ref="act_school_payment"/>
        </record>

        <record model="ir.action.act_window.view" id="act_school_payment_tree_view">
            <field name="sequence" eval="10"/>
            <field name="view" ref="school_payment_view_list1"/>
            <field name="act_window" ref="act_school_payment"/>
        </record>
        <record model="ir.action.act_window.view" id="act_school_payment_form_view">
            <field name="sequence" eval="20"/>
            <field name="view" ref="school_payment_view_form"/>
            <field name="act_window" ref="act_school_payment"/>
        </record>

        <menuitem
            sequence="20"
            parent="menu_configuration"
            action="act_school_payment"
            id="menu_school_payment"/>
        <record model="ir.ui.menu-res.group" id="menu_school_payment_group_school_admin">
            <field name="menu" ref="menu_school_payment"/>
            <field name="group" ref="group_school_admin"/>
        </record>
        <record model="ir.ui.menu-res.group" id="menu_school_payment_group_admin">
            <field name="menu" ref="menu_school_payment"/>
            <field name="group" ref="res.group_admin"/>
        </record>

<!-- SchoolAccountStatus -->

    <record model="ir.ui.view" id="school_account_status_view_form">
        <field name="model">school.account.status</field>
        <field name="type">form</field>
        <field name="name">school_account_status_form</field>
    </record>
    <record model="ir.ui.view" id="school_account_status_view_list">
        <field name="model">school.account.status</field>
        <field name="type">tree</field>
        <field name="name">school_account_status_list</field>
    </record>

    <record model="ir.action.act_window" id="act_school_account_status">
        <field name="name">Account Status</field>
        <field name="res_model">school.account.status</field>
        <field name="domain" eval="" pyson="1"/>
        <field name="context_model">school.account.status.context</field>
    </record>
    <record model="ir.action.act_window.view" id="act_school_account_status_tree_view">
        <field name="sequence" eval="10"/>
        <field name="view" ref="school_account_status_view_list"/>
        <field name="act_window" ref="act_school_account_status"/>
    </record>
    <record model="ir.action.act_window.view" id="act_school_account_status_form_view">
        <field name="sequence" eval="20"/>
        <field name="view" ref="school_account_status_view_form"/>
        <field name="act_window" ref="act_school_account_status"/>
    </record>

    <menuitem
        parent="menu_school"
        sequence="30"
        action="act_school_account_status"
        id="menu_account_status"/>
    <record model="ir.ui.menu-res.group" id="menu_account_status_group_admin">
        <field name="menu" ref="menu_account_status"/>
        <field name="group" ref="res.group_admin"/>
    </record>
    <record model="ir.ui.menu-res.group" id="menu_account_status_group_school_admin">
        <field name="menu" ref="menu_account_status"/>
        <field name="group" ref="group_school_admin"/>
    </record>

<!-- Payment Report -->
        <record model="ir.action.report" id="report_school_payment_receipt">
            <field name="name">Payment Receipt Report</field>
            <field name="model">school.payment.receipt</field>
            <field name="report_name">school.payment.receipt</field>
            <field name="report">unlimit_school_essential/report/payment_notify.fodt</field>
            <field name="extension">pdf</field>
        </record>

        <record model="ir.action.keyword" id="key_report_school_payment_receipt">
            <field name="keyword">form_print</field>
            <field name="model">school.payment.receipt,-1</field>
            <field name="action" ref="report_school_payment_receipt"/>
        </record>

<!-- Payment Report -->
        <record model="ir.action.report" id="report_school_payment">
            <field name="name">Payment Report</field>
            <field name="model">school.payment.receipt</field>
            <field name="report_name">school.payment.report</field>
            <field name="report">unlimit_school_essential/report/payment.fodt</field>
            <field name="extension">pdf</field>
        </record>

        <record model="ir.action.keyword" id="key_report_school_payment">
            <field name="keyword">form_print</field>
            <field name="model">school.payment.receipt,-1</field>
            <field name="action" ref="report_school_payment"/>
        </record>

<!-- SchoolPaymentReceipt -->

        <record model="ir.ui.view" id="school_payment_receipt_list_view">
            <field name="model">school.payment.receipt</field>
            <field name="type">tree</field>
            <field name="name">school_payment_receipt_list</field>
        </record>
        <record model="ir.ui.view" id="school_payment_receipt_form_view">
            <field name="model">school.payment.receipt</field>
            <field name="type">form</field>
            <field name="name">school_payment_receipt_form</field>
        </record>

        <record model="ir.action.act_window" id="act_school_payment_receipt">
            <field name="name">Payment Receipt</field>
            <field name="res_model">school.payment.receipt</field>
            <field name="domain" eval="" pyson="1"/>
        </record>
        <record model="ir.action.act_window.view" id="act_school_payment_receipt_tree_view">
            <field name="sequence" eval="10"/>
            <field name="view" ref="school_payment_receipt_list_view"/>
            <field name="act_window" ref="act_school_payment_receipt"/>
        </record>
        <record model="ir.action.act_window.view" id="act_school_payment_receipt_form_view">
            <field name="sequence" eval="20"/>
            <field name="view" ref="school_payment_receipt_form_view"/>
            <field name="act_window" ref="act_school_payment_receipt"/>
        </record>

        <menuitem
            parent="menu_school"
            sequence="40"
            action="act_school_payment_receipt"
            id="menu_payment_receipt"/>
        <record model="ir.ui.menu-res.group" id="menu_payment_receipt_group_admin">
           <field name="menu" ref="menu_payment_receipt"/>
           <field name="group" ref="res.group_admin"/>
        </record>
        <record model="ir.ui.menu-res.group" id="menu_payment_receipt_group_school_admin">
           <field name="menu" ref="menu_payment_receipt"/>
           <field name="group" ref="group_school_admin"/>
        </record>

<!-- SchoolPaymentReceiptEmailNotify -->
        <record model="ir.action.report" id="report_email_payment_notify">
            <field name="name">Send Notify Payment</field>
            <field name="model">school.payment.receipt</field>
            <field name="report_name">school.email.payment.notify.report</field>
            <field name="report">unlimit_school_essential/report/email_payment_notify.html</field>
            <field name="template_extension">html</field>
        </record>

    </data>
</tryton>