<?xml version="1.0"?>
<tryton>
    <data>


<!-- SchoolProductLine -->
        <record model="ir.ui.view" id="school_product_line_view_form">
            <field name="model">school.product.line</field>
            <field name="type">form</field>
            <field name="name">school_product_line_form</field>
        </record>
        <record model="ir.ui.view" id="school_product_line_view_list1">
            <field name="model">school.product.line</field>
            <field name="type">tree</field>
            <field name="name">school_product_line_list1</field>
        </record>

<!-- SchoolProduct -->
        <record model="ir.ui.view" id="school_product_view_form">
            <field name="model">school.product</field>
            <field name="type">form</field>
            <field name="name">school_product_form</field>
        </record>
        <record model="ir.ui.view" id="school_product_view_list1">
            <field name="model">school.product</field>
            <field name="type">tree</field>
            <field name="name">school_product_list1</field>
        </record>

        <record model="ir.ui.view" id="school_product_enrollment_view_form">
            <field name="model">school.product</field>
            <field name="type">form</field>
            <field name="name">school_product_enrollment_form</field>
        </record>
        <record model="ir.ui.view" id="school_product_view_list2">
            <field name="model">school.product</field>
            <field name="type">tree</field>
            <field name="name">school_product_list2</field>
        </record>

        <record model="ir.action.act_window" id="act_school_product">
            <field name="name">School Products</field>
            <field name="res_model">school.product</field>
        </record>
        <record model="ir.action.act_window.view" id="act_school_product_tree_view">
            <field name="sequence" eval="10"/>
            <field name="view" ref="school_product_view_list1"/>
            <field name="act_window" ref="act_school_product"/>
        </record>
        <record model="ir.action.act_window.view" id="act_school_product_form_view">
            <field name="sequence" eval="20"/>
            <field name="view" ref="school_product_view_form"/>
            <field name="act_window" ref="act_school_product"/>
        </record>
        
        <menuitem
            sequence="5"
            parent="menu_configuration"
            action="act_school_product"
            id="menu_school_product"/>
        <record model="ir.ui.menu-res.group" id="menu_school_product_group_school_admin">
            <field name="menu" ref="menu_school_product"/>
            <field name="group" ref="group_school_admin"/>
        </record>

<!-- SchoolLevelYear -->
        <record model="ir.ui.view" id="school_level_year_view_form">
            <field name="model">school.level.year</field>
            <field name="type">form</field>
            <field name="name">school_level_year_form</field>
        </record>
        <record model="ir.ui.view" id="school_level_year_view_list">
            <field name="model">school.level.year</field>
            <field name="type">tree</field>
            <field name="name">school_level_year_list</field>
        </record>

<!-- SchoolLevel -->
        <record model="ir.ui.view" id="school_level_view_form">
            <field name="model">school.level</field>
            <field name="type">form</field>
            <field name="name">school_level_form</field>
        </record>
        <record model="ir.ui.view" id="school_level_view_list">
            <field name="model">school.level</field>
            <field name="type">tree</field>
            <field name="name">school_level_list</field>
        </record>

        <record model="ir.action.act_window" id="act_school_level_view">
            <field name="name">Level</field>
            <field name="res_model">school.level</field>
        </record>
        <record model="ir.action.act_window.view" id="act_school_level_view_tree_view">
            <field name="sequence" eval="10"/>
            <field name="view" ref="school_level_view_list"/>
            <field name="act_window" ref="act_school_level_view"/>
        </record>
        <record model="ir.action.act_window.view" id="act_school_level_view_form_view">
            <field name="sequence" eval="20"/>
            <field name="view" ref="school_level_view_form"/>
            <field name="act_window" ref="act_school_level_view"/>
        </record>

        <menuitem
            sequence="10"
            parent="menu_configuration"
            action="act_school_level_view"
            id="menu_level"/>
        <record model="ir.ui.menu-res.group" id="menu_level_group_school_admin">
            <field name="menu" ref="menu_level"/>
            <field name="group" ref="group_school_admin"/>
        </record>

    </data>
</tryton>